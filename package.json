  "description": "SABnzbd API bindings for Node written in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/Pixselve/node-sabnzbd",
    "type": "git"
  },
  "scripts": {
